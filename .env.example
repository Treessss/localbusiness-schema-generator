# 缓存配置
# 是否使用Redis缓存 (true/false)
USE_REDIS=true

# Redis连接配置
REDIS_URL=redis://localhost:6379/0
REDIS_MAX_CONNECTIONS=100
REDIS_RETRY_ON_TIMEOUT=true
REDIS_HEALTH_CHECK_INTERVAL=30

# 并发限制配置
ENABLE_CONCURRENCY_LIMIT=true
CACHE_CONCURRENCY_LIMIT=1000
CRAWLER_CONCURRENCY_LIMIT=50
CONCURRENCY_TIMEOUT=30
CONCURRENCY_KEY_PREFIX=concurrency_limit
CLIENT_ID_HEADER=X-Client-ID